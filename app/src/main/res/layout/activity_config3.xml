<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_config3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="server.http.android.androidhttpserver.Config3"
    android:background="@drawable/dark_shiz">

    <TextView
        android:text="SSID: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="111dp"
        android:id="@+id/textView8"

        android:textColor="#fff"
        android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
        android:layout_below="@+id/textView4"
        android:layout_toStartOf="@+id/textView4"
        android:layout_toLeftOf="@+id/textView4" />

    <TextView
        android:text="Enter details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"

        android:textColor="#fff"
        android:textAppearance="@style/TextAppearance.AppCompat.Large.Inverse"
        android:layout_marginTop="63dp"
        android:textSize="25dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Password:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"

        android:textColor="#fff"
        android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
        android:layout_centerVertical="true"
        android:layout_alignEnd="@+id/textView8"
        android:layout_alignRight="@+id/textView8" />

    <TextView
        android:text=" IP Address: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"

        android:textColor="#fff"
        android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
        android:layout_below="@+id/textView6"
        android:layout_alignEnd="@+id/textView6"
        android:layout_marginTop="47dp"
        android:layout_alignRight="@+id/textView6" />

    <TextView
        android:text="Port:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"

        android:textColor="#fff"
        android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
        android:layout_marginTop="54dp"
        android:layout_below="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignRight="@+id/textView5" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:textColor="#000"
        android:id="@+id/pas"
        android:background="@drawable/rounded"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/mip"
        android:layout_alignLeft="@+id/mip" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:enabled="false"
        android:text="8080"
        android:textColor="#000"
        android:background="@drawable/rounded"
        android:id="@+id/por"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/sid"
        android:layout_alignLeft="@+id/sid" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/sid"
        android:background="@drawable/rounded"
        android:textColor="#000"
        android:layout_marginEnd="20dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:background="@drawable/rounded"
        android:id="@+id/mip"
        android:text="192.168.4.1"
        android:textColor="#000"
        android:enabled="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium.Inverse"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignStart="@+id/por"
        android:layout_alignLeft="@+id/por" />

    <Button
        android:text="Finish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_shape"
        android:id="@+id/button3"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/por"
        android:layout_marginBottom="35dp"
        android:layout_alignRight="@+id/por" />


</RelativeLayout>
